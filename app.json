{
  "expo": {
    "name": "musico",
    "slug": "musico",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "musico",
    "androidStatusBar": {
      "barStyle": "light-content"
    },
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#00000000",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#632C46"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#632C46"
      },
      "package": "com.devvie.musico"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8608ca11-51dc-4edb-8d9c-72b7976f027b"
      }
    }
  }
}
